# Description: Python implementation of SSHv2 protocol
# URL: http://www.paramiko.org/
# Maintainer: Slawomir Gonet, slawek at otwiera dot cz
# Depends on: python python-setuptools

name=python-paramiko
_shortname=paramiko
version=2.1.2
release=1
source=(https://sources.voidlinux.eu/$name-$version/$_shortname-$version.tar.gz)

build() {
	cd $_shortname-$version
	python setup.py build
	python setup.py install --prefix=/usr --root $PKG
}
